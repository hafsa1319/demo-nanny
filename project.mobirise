"0.1\" value=\"0.7\" condition=\"overlay && bg.type !== 'color'\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n\n\n    <div class=\"container-fluid align-center\">\n        <div class=\"row content-row\">\n            <div class=\"col-lg-6 col-md-12 align-center imageContainer\">\n                <img src=\"@PROJECT_PATH@/assets/images/father2.jpg\" alt=\"Mobirise\">\n                <div class=\"blue-block\">\n                  <div class=\"wrapper\">\n                      <h3 class=\"mbr-text mbr-bold mbr-fonts-style group-title blue-title\" mbr-theme-style=\"display-5\" data-app-selector=\".group-title\" mbr-if=\"showTitle\">Happy Hours</h3>\n                      <p class=\"mbr-text text-blue timetable mbr-normal links mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".links, .navbar-brand\">\n                          Mon - Fri : 8am - 7pm <br>\n                          Saturday : 9am - 4pm <br>\n                          Sunday : Closed <br>\n                      </p>\n                  </div>\n              </div>\n            </div>\n            <div class=\"col-md-12 col-lg-6 py-4 m-auto align-left right-wrapper\">\n                <h1 class=\"mbr-section-title mbr-fonts-style\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title\"><span>Nanny Services</span></h1>\n                <h3 class=\"mbr-section-subtitle mbr-fonts-style\" mbr-theme-style=\"display-2\" mbr-if=\"showSubTitle\" data-app-selector=\".mbr-section-subtitle\"><span>Evening Nanny</span></h3>\n                <p class=\"mbr-text mbr-light align-left mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".right-wrapper .mbr-text, .mbr-section-btn\">\n                    Lorem ipsum dolor sit amet, consectetur adipiscing elit. Donec dui enim, pharetra at auctor vitae.\n                </p>\n                <div mbr-buttons mbr-theme-style=\"display-7\" class=\"button-svg mbr-section-btn align-left\" mbr-if=\"showButtons\" data-toolbar=\"-mbrBtnMove\"><a class=\"btn btn-primary\" href=\"https://mobiri.se\" data-app-placeholder=\"Type Text\">Read More<svg class=\"wgl-dashes svg1 inner-dashed-border animated-dashes\"><rect x=\"2px\" y=\"2px\" rx=\"25px\" ry=\"25px\" width=\"100%\" height=\"100%\"></rect></svg>\n                </a></div>\n              </div>\n            </div>\n\n\n        </div>\n\n\n\n    \n    <div mbr-if=\"showArrow\" class=\"mbr-arrow hidden-sm-down\" aria-hidden=\"true\">\n        <a href=\"#next\">\n            <i class=\"mbri-down mbr-iconfont\"></i>\n        </a>\n    </div>\n</section>",
