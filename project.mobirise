          "_name": "features8",
          "_customHTML": "<section group=\"Headers\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" mbr-class=\"{\n    'mbr-fullscreen': fullScreen,\n    'mbr-parallax-background': bg.parallax}\">\n\n    <mbr-parameters>\n        <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"25\" step=\"1\" value=\"6\" condition=\"fullScreen == false\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"25\" step=\"1\" value=\"5\" condition=\"fullScreen == false\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubTitle\" checked>\n        <input type=\"checkbox\" title=\"Text\" name=\"showText\">\n        <input type=\"checkbox\" title=\"Experience\" name=\"showExperience\" checked>\n        <input type=\"checkbox\" title=\"Email\" name=\"showEmail\" checked>\n        <input type=\"checkbox\" title=\"Phone\" name=\"showPhone\" checked>\n        <input type=\"checkbox\" title=\"Fax\" name=\"showFax\" checked>\n        \n        <input inline type=\"range\" title=\"Icons\" name=\"iconsCount\" min=\"0\" max=\"8\" step=\"1\" value=\"3\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"@PROJECT_PATH@/assets/images/children.jpg\" parallax>\n            <input type=\"color\" title=\"Color\" value=\"#f2f6ff\" selected>\n            <input type=\"video\" title=\"Video\" value=\"https://vimeo.com/201169087\n            \">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.7\" condition=\"overlay && bg.type !== 'color'\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div class=\"container-fluid align-center\">\n        <div class=\"row wrapper-block\">\n            <div class=\"col-lg-6 col-md-12 align-center imageContainer\">\n                <img src=\"@PROJECT_PATH@/assets/images/family1.jpg\" alt=\"Mobirise\">\n            </div>\n\n            <div class=\"col-md-12 col-lg-6 py-4 m-auto right-wrapper\">\n                <h1 class=\"mbr-section-title align-left mbr-fonts-style\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\">Anna Byrne</h1>\n                <h3 class=\"mbr-section-subtitle align-left  mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showSubTitle\" data-app-selector=\".mbr-section-subtitle\">Full-time Nanny</h3>\n                <p mbr-if=\"showExperience\" class=\"experience mbr-fonts-style\" mbr-theme-style=\"display-7\"> \n                  <span class=\"title2\" data-app-selector=\".title2\">Experience:</span> 12 Years \n                </p>\n                <p mbr-if=\"showEmail\" class=\"E-mail pb-1 mbr-fonts-style\" mbr-theme-style=\"display-7\"> \n                  <span class=\"title2\">E-mail:</span> info@site.com\n                </p>\n                <p mbr-if=\"showPhone\" class=\"Phone pb-1 mbr-fonts-style\" mbr-theme-style=\"display-7\"> \n                  <span class=\"title2\">Phone:</span> +123 456 789 \n                </p>\n                <p mbr-if=\"showFax\" class=\"Fax pb-1 mbr-fonts-style\" mbr-theme-style=\"display-7\"> \n                  <span class=\"title2\">Fax:</span>  +123 456 789\n                </p>\n                <p class=\"mbr-text mbr-light align-left mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".mbr-text, .mbr-section-btn\">\n                    Lorem ipsum dolor sit amet, consectetur adipiscing elit. Donec dui enim, pharetra at auctor vitae.\n                </p>\n                <div class=\"social-list pt-2 align-left\" mbr-if=\"iconsCount > 0\">\n                  <div class=\"soc-item soc-item1\" mbr-if=\"iconsCount>0\">\n                      <a href=\"https://twitter.com/mobirise\" target=\"_blank\">\n                          <span mbr-icon class=\"mbr-iconfont mbr-iconfont-social socicon-twitter socicon\"></span>\n                      </a>\n                  </div>\n                  <div class=\"soc-item soc-item2\" mbr-if=\"iconsCount>1\">\n                      <a href=\"https://www.facebook.com/pages/Mobirise/1616226671953247\" target=\"_blank\">\n                          <span mbr-icon class=\"mbr-iconfont mbr-iconfont-social socicon-facebook socicon\"></span>\n                      </a>\n                  </div>\n                  <div class=\"soc-item soc-item3\" mbr-if=\"iconsCount>2\">\n                      <a href=\"https://www.youtube.com/c/mobirise\" target=\"_blank\">\n                          <span mbr-icon class=\"mbr-iconfont mbr-iconfont-social socicon-youtube socicon\"></span>\n                      </a>\n                  </div>\n                  <div class=\"soc-item soc-item1\" mbr-if=\"iconsCount>3\">\n                      <a href=\"https://instagram.com/mobirise\" target=\"_blank\">\n                          <span mbr-icon class=\"mbr-iconfont mbr-iconfont-social socicon-vkontakte socicon\"></span>\n                      </a>\n                  </div>\n                  <div class=\"soc-item soc-item2\" mbr-if=\"iconsCount>4\">\n                      <a href=\"https://plus.google.com/u/0/+Mobirise\" target=\"_blank\">\n                          <span mbr-icon class